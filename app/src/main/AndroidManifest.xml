<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mdsuhelrana.surveyproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/fullscreenWindow"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".BasicInfoActivity" />
        <activity android:name=".question.App1qsnActivity" />
        <activity android:name=".question.App2qsnActivity" />
        <activity android:name=".question.App3qsnActivity" />
        <activity android:name=".question.App4qsnActivity" />
        <activity android:name=".question.App5qsnActivity" />
        <activity android:name=".UserActivity"
          android:screenOrientation="landscape" />
        <activity android:name=".AdminActivity"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>